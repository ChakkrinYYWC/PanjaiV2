            }\n              }\n              \n              else {\n                return (\n                  <li className=\"itemlist\" key={index}>\n                    {/* <a href={\"#\" + item.id}> */}\n                    <a className={item.cName} href={item.href}>\n                      {item.title}\n                    </a>\n                    {/* </a> */}\n                  </li>\n                );\n              }\n            })}\n\n            {/* ============================================================================== */}\n            {currentUser != \"admin\" ? (\n              <span></span>\n            ) : (\n              <span class=\"drop\">\n                <DropdownButton id=\"dropdown-item-button\" title=\" admin\">\n                  <span className=\"lover\">\n                    <Dropdown.Item as=\"button\">\n                      <div onClick={() => this.BlackListOn()} className=\"love\">\n                        <a>Blacklist</a>\n                      </div>\n                    </Dropdown.Item>\n                    <Dropdown.Item as=\"button\">\n                      <div onClick={() => this.UserOn()} className=\"love\">\n                        <a>User</a>\n                      </div>\n                    </Dropdown.Item>\n                    <Dropdown.Item as=\"button\">\n                      <div onClick={() => this.ReportOn()} className=\"love\">\n                        <a>Report</a>\n                      </div>\n                    </Dropdown.Item>\n                  </span>\n                </DropdownButton>\n              </span>\n              // <span>\n              //   <div>\n              //     <button onClick={() => this.BlackListOn()} ><a>&nbsp;Blacklist&nbsp;</a></button>\n              //     <button><a>&nbsp;User&nbsp;</a></button>\n              //     <button><a>&nbsp;Report&nbsp;</a></button>\n              //   </div>\n              // </span>\n            )}\n\n            {/* =======================