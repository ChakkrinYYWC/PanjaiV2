)\n    }\n\n/*-----------------------------------------------------------*/\n    return (\n        <div className=\"grid-container\" >\n            <div className=\"item1\">\n                <div className=\"image\">\n                    <img src={loginImg} />\n                </div>\n            </div>\n            <div className=\"item2\">\n                <h3>เข้าสู่ระบบ</h3><br/>\n                <form>\n                    <div className=\"form-group\">\n                        <label>ชื่อผู้ใช้:</label><br />\n                        <input\n                        type=\"text\"\n                        placeholder=\"ชื่อผู้ใช้\"\n                        onChange={(event) =>{\n                            setUsername(event.target.value)\n                        }}\n                        >\n                        </input>\n                    </div>\n                    <div className=\"form-group\">\n                        <label>รหัสผ่าน:</label><br />\n                        <input\n                        type=\"password\"\n                        placeholder=\"รหัสผ่าน\"\n                        onChange={(event) =>{\n                            setPassword(event.target.value)\n                        }}\n                        >\n                        </input>\n                    </div>\n                    <div className=\"button-login\">\n                        <button type='submit' className=\"btn btn-lg\" onClick={uploadFile}>เข้าสู่ระบบ</button>\n                    </div>\n                </form>\n                <div>\n                <br />\n                    <h5>\n                        <a href=\"/Register\">ลืมรหัสผ่าน</a> | <a href=\"/Register\">สมัครสมาชิก</a>\n                    </h5>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default LoginFrom;\n//export default connect(mapStateToProps, mapActionToProps)(withStyles(styles)(PostPanjaiForm));","/Users/roong/roong/PanjaiV2/client/src/components/