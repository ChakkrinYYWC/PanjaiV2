\n    };\n\n\n\n    return (\n        <>\n            <If condition={currentUser == 'admin'}>\n                <Then>\n                    <Provider store={store}>\n                        <Fab size=\"small\" color=\"primary\" aria-label=\"add\" onClick={handleClickOpen} >\n                            <AddIcon />\n                        </Fab>\n\n                        <Dialog onClose={handleClose} aria-labelledby=\"customized-dialog-title\" open={open}>\n                            <DialogTitle id=\"customized-dialog-title\" onClose={handleClose}>\n                                Post Foundation\n                        </DialogTitle>\n                            <PostFDT {...{ currentId, setCurrentId }} />\n                            <ButterToast position={{ vertical: POS_TOP, horizontal: POS_RIGHT }} />\n                        </Dialog>\n                    </Provider>\n                    <FDTform />\n                    <PageFoundation />\n                </Then>\n                <Else>\n                    <FDTform />\n                    <PageFoundation />\n                </Else>\n            </If>\n        </>\n\n    );\n}\n\nexport default Foundation;","/Users/roong/roong/PanjaiV2/client/src/pages/Too-panjai.jsx",["149","150","151","152"],"import Axios from 'axios';\nimport React from 'react';\nimport { useState } from 'react';\nimport { Provider } from \"react-redux\";\nimport PostPanjai from \"../components/PostPanjai/PostPanjai\";\nimport { store } from \"../action/store\";\nimport { Container, AppBar, Typography } from \"@material-ui/core\";\nimport ButterToast, { POS_RIGHT, POS_TOP } from \"butter-toast\";\nimport { createMuiTheme } from '@material-ui/core/styles';\nimport { makeStyles } from '@material-ui/core/styles';\n\nconst useStyles = makeStyles({\n\n    primary: {\n        background: 'white',\n        padding: '10px 10px 10px 20px',\n        marginBlock: '15px',\n        // boxShadow: '0 5px 6px 5px rgba(187, 130, 44, 0.925)',     \n    },\n\n    toopanjaitext: {\n        color: ' rgba(141, 90, 18, 0.925)',\n        fontFamily: 'mali',\n        fontSize: '30px',\n        fontWeight: 'bold'\n       \n    }\n\n\n});\n\nfunction Too_panjai() {\n    const classes = useStyles();\n    return (\n        <Provider store={store}>\n            <Container maxWidth=\"lg\">\n                <div position=\"static\" className={classes.primary}>\n                    <Typography\n                        variant=\"h2\"\n                        align=\"center\">\n                        {/* ข้อความ */}\n                        <span className={classes.toopanjaitext}>ตู้ ปั น ใ จ</span>\n                    </Typography>\n                </div>\n                <PostPanjai />\n                <ButterToast position={{ vertical: POS_TOP, horizontal: POS_RIGHT }} />\n            </Container>\n        </Provider>\n    );\n}\n\nexport default Too_panjai;","/Users/roong/roong/PanjaiV2/client/src/pages/userInformation.jsx",[],"/Users/roong/roong/PanjaiV2/client/src/pages/register.jsx",["153","154","155","156"],"import React from 'react';\nimport RegisterFrom from \"../components/authen/registerForm\";\nimport { Container, AppBar, Typography } from \"@material-ui/core\";\nimport { makeStyles } from '@material-ui/core/styles';\n\n// const useStyles = makeStyles({\n\n//     primary: {\n//         background: 'white',\n//         padding: '10px 10px 10px 20px',\n//         marginBlock: '15px'\n//     },\n//     toopanjaitext: {\n//         color: 'rgba(187, 130, 44, 0.925)',\n//         fontFamily: 'mali',\n//         fontSize: '36px'\n//     }\n\n\n// });\n\nfunction Register() {\n    return (\n        <RegisterFrom />\n    );\n}\n\nexport default Register;","/Users/roong/roong/PanjaiV2/client/src/pages/Homepage.jsx",["157","158","159","160","161","162","163","164"],"import Axios from 'axios';\nimport React from 'react';\nimport { useState } from 'react';\nimport Foundation from './Foundation'\nimport Home from '../components/ho