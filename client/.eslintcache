lose ? (\r\n                <IconButton aria-label=\"close\" className={classes.closeButton} onClick={onClose}>\r\n                    <CloseIcon />\r\n                </IconButton>\r\n            ) : null}\r\n        </MuiDialogTitle>\r\n\r\n\r\n    );\r\n});\r\n\r\n\r\n\r\nfunction Foundation({ classes, ...props }) {\r\n\r\n    const [open, setOpen] = React.useState(false);\r\n    const [currentId, setCurrentId] = useState(0)\r\n    const currentUser = localStorage.getItem('currentUser')\r\n\r\n    const handleClickOpen = () => {\r\n        setOpen(true);\r\n    };\r\n    const handleClose = () => {\r\n        setOpen(false);\r\n    };\r\n\r\n\r\n\r\n    return (\r\n        <>\r\n            <If condition={currentUser == 'admin'}>\r\n                <Then>\r\n                    <Provider store={store}>\r\n                        <Fab size=\"small\" color=\"primary\" aria-label=\"add\" onClick={handleClickOpen} >\r\n                            <AddIcon />\r\n                        </Fab>\r\n\r\n                        <Dialog onClose={handleClose} aria-labelledby=\"customized-dialog-title\" open={open}>\r\n                            <DialogTitle id=\"customized-dialog-title\" onClose={handleClose}>\r\n                                Post Foundation\r\n                        </DialogTitle>\r\n                            <PostFDT {...{ currentId, setCurrentId }} />\r\n                            <ButterToast position={{ vertical: POS_TOP, horizontal: POS_RIGHT }} />\r\n                        </Dialog>\r\n                        <PageFoundation {...{ currentId, setCurrentId }}/>\r\n                    </Provider>\r\n\r\n                </Then>\r\n                <Else>\r\n                    <Provider store={store}>\r\n                        <PageFoundation {...{ currentId, setCurrentId }}/>\r\n                    </Provider>\r\n                </Else>\r\n            </If>\r\n        </>\r\n\r\n    );\r\n}\r\n\r\nexport default Foundation;","C:\\Users\\ASUS\\Desktop\\PanjaiV2\\client\\src\\action\\postFDT.js",[],"C:\\Users\\ASUS\\Desktop\\PanjaiV2\\client\\src\\reducers\\postFDT.js",["228","229","230"],"import { ACTION_TYPES } from '../action/postFDT'\r\nconst initialState = {\r\n    list: []\r\n}\r\n\r\nexport const postFDT = (state = initialState, action) => {\r\n    switch (action.type) {\r\n        case ACTION_TYPES.FETCH_ALL:\r\n            return {\r\n                ...state,\r\n                list: [...action.payload]\r\n            }\r\n        case ACTION_TYPES.FETCH_ById:\r\n            return {\r\n                ...state,\r\n                list: state.list.map(x => x._id == action.payload._id ? action.payload : x)\r\n            }\r\n        case ACTION_TYPES.CREATE:\r\n            return {\r\n                ...state,\r\n                list: [...state.list, action.payload]\r\n            }\r\n        case ACTION_TYPES.UPDATE:\r\n            return {\r\n                ...state,\r\n                list: state.list.map(x => x._id == action.payload._id ? action.payload : x)\r\n            }\r\n\r\n        case ACTION_TYPES.DELETE:\r\n            return {\r\n                ...state,\r\n                list: state.list.filter(x => x._id != action.payload)\r\n            }\r\n\r\n        default:\r\n            return state;\r\n    }\r\n}","C:\\Users\\ASUS\\Desktop\\PanjaiV2\\client\\src\\pages\\userInformation.jsx",[],"C:\\Users\\ASUS\\Desktop\\PanjaiV2\\client\\src\\components\\authen\\loginFrom.js",["231","232","233","234","235","236","237","238","239","240","241","242"],"C:\\Users\\ASUS\\Desktop\\PanjaiV2\\client\\src\\components\\authen\\registerForm.js",["243","244"],"import Axios from 'axios';\r\nimport React from 'react';\r\nimport { useState } from 'react';\r\nimport { Link, Redirect } from 'react-router-dom';\r\nimport loginImg from \"../img/login.svg\";\r\nimport \"./login.css\";\r\n\r\nfunction RegisterFrom() {\r\n/*------------------------------------------------------------*/\r\n    const [usernam